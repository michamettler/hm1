   "source": [
    "import sympy as sp\n",
    "\n",
    "from lgs_iter_jacobi import jacobi\n",
    "\n",
    "x = sp.symbols('x')\n",
    "\n",
    "a = sp.Matrix([\n",
    "    [8, 5, 2],\n",
    "    [5, 9, 1],\n",
    "    [4, 2, 7],\n",
    "])\n",
    "\n",
    "b = sp.Matrix([19, 5, 34])\n",
    "x0 = sp.Matrix([1, -1, 3])\n",
    "\n",
    "x = jacobi(a, b, x0, 5, precision=6).evalf()\n",
    "print(x)"
   ]
  }
