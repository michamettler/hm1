    "import sympy as sp\n",
    "\n",
    "from eigen_von_mises import von_mises\n",
    "\n",
    "a = sp.Matrix([\n",
    "    [13, -4],\n",
    "    [30,-9],\n",
    "])\n",
    "\n",
    "v0 = sp.Matrix([\n",
    "    [1],\n",
    "    [0]\n",
    "])\n",
    "\n",
    "eigen_vec, eigen_val = von_mises(a, v0, iterations=40, precision=8)"
   ]
  }
